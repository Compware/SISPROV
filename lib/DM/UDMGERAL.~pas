procedure TDMGERAL.TB_PACIENTEFL_CRIANCA_EXPOSTAValidate(Sender: TField);
var
  nrProcessoMae: Variant;
  prefixoNrProc: String;
  qy: TIBQuery;
  i: integer;
  procurando: boolean;
begin
  if (TB_PACIENTEFL_CRIANCA_EXPOSTA.AsString = cvSim) and
      (TB_PACIENTECD_PACIENTE_MAE.AsString = '') then
  begin
     ShowMessage('Antes de marcar como criana exposta  preciso selecionar a me pelo boto "Procurar Me"');
     Abort;
  end;


  if (TB_PACIENTEFL_CRIANCA_EXPOSTA.AsString = cvSim) and
      (TB_PACIENTENR_PROCESSO_CRIANCA_EXP.AsString = '') and
      (TB_PACIENTECD_PACIENTE_MAE.AsString <> '') then
  begin
    //nrProcessoMae := PegaValor('NR_PROCESSO', 'PACIENTE', ['CD_PACIENTE'], [TB_PACIENTECD_PACIENTE_MAE.AsInteger]);
    //if not VarIsNull(nrProcessoMae) then
    //  TB_PACIENTENR_PROCESSO_CRIANCA_EXP.AsString := IntToStr(nrProcessoMae) + '-';

    nrProcessoMae := PegaValor('NR_PROCESSO', 'PACIENTE', ['CD_PACIENTE'], [TB_PACIENTECD_PACIENTE_MAE.AsInteger]);

    if VarIsNull(nrProcessoMae) then
    begin
      ShowMessage('O cadastro da me no tem nmero de processo');
      Abort;
    end;

    prefixoNrProc := IntToStr(nrProcessoMae) + '-';

    qy := TIBQuery.Create(nil);
    with qy do
    begin
      try
        Database := dm.DB;
        Transaction := dm.TS;
        SQl.Add('SELECT NR_PROCESSO_CRIANCA_EXP FROM PACIENTE');
        SQl.Add('WHERE NR_PROCESSO_CRIANCA_EXP like :NR_PROCESSO_CRIANCA_EXP' );
        qy := ExecutaSQL(qy, ['NR_PROCESSO_CRIANCA_EXP'], [prefixoNrProc+'%']);


        if RecordCount > 0 then
        begin
          i := 1;
          procurando := true;
          while procurando do
          begin
            if not Locate('NR_PROCESSO_CRIANCA_EXP', prefixoNrProc+IntToStr(i), []) then
            begin
              procurando := false;
              TB_PACIENTENR_PROCESSO_CRIANCA_EXP.AsString := prefixoNrProc + IntToStr(i);
            end
            else
              Inc(i);
          end;
        end else
        begin
          TB_PACIENTENR_PROCESSO_CRIANCA_EXP.AsString := prefixoNrProc + '1';
        end;
      finally
        Close;
        Free;
      end;
    end;
  end;
end;

procedure TDMGERAL.TB_PACIENTENR_PROCESSO_CRIANCA_EXPValidate(
  Sender: TField);
//var
  //qyConfereNrProcesso : TIBQuery;
begin
 { if TB_PACIENTENR_PROCESSO_CRIANCA_EXP.AsString <> '' then
  begin
    try
      qyConfereNrProcesso := TIBQuery.Create(nil);
      with qyConfereNrProcesso do
      begin
        Database := DM.DB;
        Transaction := DM.TS;

        SQL.Add('SELECT 1 FROM PACIENTE');
        SQL.Add('WHERE CD_PACIENTE<>:CD_PACIENTE');
        SQL.Add('AND NR_PROCESSO_CRIANCA_EXP = :NR_PROCESSO_CRIANCA_EXP');

        Prepare;

        ParamByName('CD_PACIENTE').AsInteger := TB_PACIENTECD_PACIENTE.AsInteger;
        ParamByName('NR_PROCESSO_CRIANCA_EXP').AsString := TB_PACIENTENR_PROCESSO_CRIANCA_EXP.AsString;

        Open;

        if not IsEmpty then
        begin
          ShowMessage('J existe um cadastro com esse Nr. Processo Criana Exposta');
          Abort;
        end;

      end;
    finally
      qyConfereNrProcesso.Close;
      FreeAndNil(qyConfereNrProcesso);
    end;
  end; }
end;

procedure TDMGERAL.TB_PACIENTEAfterEdit(DataSet: TDataSet);
begin
  AtualizaDataNascimento;
end;

procedure TDMGERAL.AtualizaDataNascimento;
var idade, meses : integer;
begin
  if ((TB_PACIENTEDT_NASCIMENTO.AsString <> '')
      and (TB_PACIENTE.State = dsEdit)) then
  begin
    idade := CalculoIdade(TB_PACIENTEDT_NASCIMENTO.AsDateTime);
    TB_PACIENTEIDADE.AsInteger := idade;
    if TB_PACIENTEIDADE.AsInteger = 0 then
      begin
        meses := CalculoMeses(TB_PACIENTEDT_NASCIMENTO.AsDateTime);
        TB_PACIENTEMESES.AsInteger := meses;
      end
      else
        TB_PACIENTEMESES.AsInteger := 0;
  end;
end;

end.
