procedure TDMGERAL.RemarcacaoAgenda;
var
 cd_paciente,cd_unidade,cd_medico,
 tp_esquema,vl_cd4,fl_realizado : integer;
 dt_agenda :TDateTime;
 ds_esquema :string;
begin
  cd_paciente := DMGERAL.TB_AGENDACD_PACIENTE.AsInteger;
  cd_unidade := DMGERAL.TB_AGENDACD_UNIDADE.AsInteger;
  cd_medico := DMGERAL.TB_AGENDACD_MEDICO.AsInteger;
  tp_esquema := DMGERAL.TB_AGENDATP_ESQUEMA.AsInteger;
  ds_esquema := DMGERAL.TB_AGENDADS_ESQUEMA.AsString;
  vl_cd4 := DMGERAL.TB_AGENDAVL_CD4.AsInteger;
  fl_realizado := DMGERAL.TB_AGENDAFL_REALIZADO.AsInteger;
  dt_agenda := DMGERAL.TB_AGENDADT_REMARCACAO.AsDateTime;

  DMGERAL.TB_AGENDA.Insert;

  DMGERAL.TB_AGENDACD_PACIENTE.AsInteger := cd_paciente;
  DMGERAL.TB_AGENDACD_UNIDADE.AsInteger := cd_unidade;
  DMGERAL.TB_AGENDACD_MEDICO.AsInteger := cd_medico;
  DMGERAL.TB_AGENDATP_ESQUEMA.AsInteger := tp_esquema;
  DMGERAL.TB_AGENDADS_ESQUEMA.AsString := ds_esquema;
  DMGERAL.TB_AGENDAVL_CD4.AsInteger := vl_cd4;
  DMGERAL.TB_AGENDAFL_REALIZADO.AsInteger := fl_realizado;
  DMGERAL.TB_AGENDADT_AGENDA.AsDateTime := dt_agenda;

  DMGERAL.TB_AGENDA.Post;

end;

