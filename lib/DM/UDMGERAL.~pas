procedure TDMGERAL.GetVersaoUnidadesWeb;
var sFile:string;  str:TStrings;
begin
  //Gera o arquivos.xml
  str := TStringList.Create;
  str.Text := Get(sDominio + '/dbUnidade.php?op=S');
  sFile := sPathTemp + 'Unidades.xml';
  str.SaveToFile(sFile);
  cds.LoadFromFile(sFile);
  TB_UNIDADE.Open;
  while not cds.Eof do
  begin
    if TB_UNIDADE.Locate('CD_UNIDADE',cds.FieldByName('UNIDADE').AsInteger,[]) then
    begin
        TB_UNIDADE.Edit;
        TB_UNIDADEVL_VERSAO.AsString := cds.FieldByName('VERSAO').AsString;
        TB_UNIDADE.Post;
    end;
    cds.Next;
  end;
  TB_UNIDADE.Close;
  str.Free;
end;

