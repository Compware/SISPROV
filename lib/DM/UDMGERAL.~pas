procedure TDMGERAL.TB_AGENDABeforePost(DataSet: TDataSet);
var dt:TDateTime;
begin
  dt := dm.GetDateTime;
  if (TB_AGENDAFL_CONFIRMADO.AsInteger = 1) then
     TB_AGENDADT_CONFIRMACAO.AsDateTime := dt;

  TB_AGENDACD_USU_ALTERACAO.AsInteger := dm.qyLoginCD_USUARIO.AsInteger;
  TB_AGENDADT_ALTERACAO.AsDateTime := dt;

  if TB_AGENDANR_LAB.AsString = '' then
    TB_AGENDANR_LAB.AsInteger := dm.GeraCodigo('gen_numero_lab');

{TODO: Someente para DEBUG Comentar
  if TB_AGENDACD_MEDICO.AsString = prmMedicoIntegraLab then
    if (TB_AGENDAFL_LAB.NewValue = Null) then
      dmLab.IncluiAgenda(NUMERO_INDEFINIDO);
}
end;

