procedure TDMGERAL.TB_AGENDACalcFields(DataSet: TDataSet);
begin
  case TB_AGENDAFL_REALIZADO.AsInteger of
    0:TB_AGENDADS_REALIZADO.AsString := 'NO';
    1:TB_AGENDADS_REALIZADO.AsString := 'SIM';
  end;

  case TB_AGENDAFL_REMARCADO.AsInteger of
    0:TB_AGENDADS_REMARCADO.AsString := 'NO';
    1:TB_AGENDADS_REMARCADO.AsString := 'SIM';
  end;

  case TB_AGENDAFL_CONFIRMADO.AsInteger of
    0:TB_AGENDADS_CONFIRMADO.AsString := 'NO';
    1:BEGIN
        TB_AGENDADS_CONFIRMADO.AsString := 'SIM';
        TB_AGENDACALC_STATUS.AsString := 'Confirmado';
      END;
  end;

  case TB_AGENDAFL_TRANSFERIDO.AsInteger of
    0:TB_AGENDADS_TRANSFERIDO.AsString := 'NO';
    1:TB_AGENDADS_TRANSFERIDO.AsString := 'SIM';
  end;

  TB_AGENDAFL_DIGITAL.AsInteger := cvFlNao;

  if TB_AGENDADIGITAL.AsInteger = cvFlSim then
    TB_AGENDAFL_DIGITAL.AsInteger := cvFlSim;
end;

procedure TDMGERAL.TB_AGENDAAfterInsert(DataSet: TDataSet);
begin
  TB_AGENDACD_AGENDA.AsInteger := dm.GeraCodigo('GEN_AGENDA');
  TB_AGENDAFL_REMARCADO.AsInteger := 0;
  TB_AGENDAFL_REALIZADO.AsInteger := 0;
  TB_AGENDAFL_CONFIRMADO.AsInteger := 0;
  TB_AGENDAFL_ESQUEMA.AsInteger := 0;
  TB_AGENDAFL_LAB.AsString := 'N';
  TB_AGENDAFL_RX.AsString := 'N';
  TB_AGENDACD_UNIDADE.AsString := prmUnidade;
  TB_AGENDADS_UNIDADE.AsString := sUnidadeDefautDescricao;
  TB_AGENDA.FieldByName('CD_UND').AsString := prmUnidade;
  TB_AGENDACD_USU_INCLUSAO.AsInteger := dm.qyLoginCD_USUARIO.AsInteger;
  TB_AGENDADT_INCLUSAO.AsDateTime := dm.GetDateTime;
end;

