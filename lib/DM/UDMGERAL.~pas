procedure TDMGERAL.TB_OCUPACAOAfterInsert(DataSet: TDataSet);
begin
  if (prmSistemaUnidades<>cvSim) then
  begin
    TB_OCUPACAO.FieldByName('CD_UND').AsString := prmUnidade;
  end
  else
  begin
    TB_OCUPACAO.FieldByName('CD_UND').AsString := prmUnidadeCentral;
  end;
end;

