procedure TDMGERAL.TB_RESULTADO_CD4CalcFields(DataSet: TDataSet);
begin
  if TB_RESULTADO_CD4NR_CD3.AsFloat > 0 then
    TB_RESULTADO_CD4NR_PRCD4.AsFloat := (TB_RESULTADO_CD4NR_CD4.AsFloat * 100)/
       TB_RESULTADO_CD4NR_CD3.AsFloat;

  if TB_RESULTADO_CD4NR_CD3.AsFloat > 0 then
    TB_RESULTADO_CD4NR_PRCD8.AsFloat := (TB_RESULTADO_CD4NR_CD8.AsFloat * 100)/
       TB_RESULTADO_CD4NR_CD3.AsFloat;

  if TB_RESULTADO_CD4NR_CD8.AsFloat > 0 then
    TB_RESULTADO_CD4NR_RCD4CD8.AsFloat := (TB_RESULTADO_CD4NR_CD4.AsFloat /
       TB_RESULTADO_CD4NR_CD8.AsFloat);
end;

