    TB_NOTIFICACAODT_EVOLUCAO.Clear;
  if TB_NOTIFICACAODT_EVOLUCAO_2.AsString = '30/12/1899' then
    TB_NOTIFICACAODT_EVOLUCAO_2.Clear;
