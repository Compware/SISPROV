procedure TDMGERAL.ProtocolaEnvio(DataSet: TIBDataSet; nTipo:Integer; sOP,sArq:String);
begin

  TB_PROTOCOLO_ENVIO.Close;
  TB_PROTOCOLO_ENVIO.ParamByName('CD_UNIDADE').AsInteger := DataSet.FieldByName('CD_UNIDADE').AsInteger;
  TB_PROTOCOLO_ENVIO.ParamByName('CD_PROVINCIA').AsInteger := DataSet.FieldByName('CD_PROVINCIA').AsInteger;
  TB_PROTOCOLO_ENVIO.ParamByName('CD_MUNICIPIO').AsInteger := DataSet.FieldByName('CD_MUNICIPIO').AsInteger;
  TB_PROTOCOLO_ENVIO.ParamByName('VL_MESANO').AsString := DataSet.FieldByName('VL_MESANO').AsString;
  TB_PROTOCOLO_ENVIO.ParamByName('TP_PROTOCOLO').AsInteger := nTipo;
  {TODO: Aqui precisa ter o TIPO de PROTOCOLO tambem pois os campos chaves podem ser de outros relatorios tambem}
  TB_PROTOCOLO_ENVIO.Open;

  //if TB_PROTOCOLO_ENVIO.IsEmpty then
    TB_PROTOCOLO_ENVIO.Insert;
  //else
  //  TB_PROTOCOLO_ENVIO.Edit;

  TB_PROTOCOLO_ENVIOCD_UND.AsString := prmUnidade;
  TB_PROTOCOLO_ENVIOTP_PROTOCOLO.AsInteger := nTipo;
  TB_PROTOCOLO_ENVIOCD_REGISTRO_GERAL.AsInteger := DataSet.FieldByName('CD_REGISTRO_GERAL').AsInteger;
  TB_PROTOCOLO_ENVIOCD_UNIDADE.AsInteger := DataSet.FieldByName('CD_UNIDADE').AsInteger;
  TB_PROTOCOLO_ENVIOCD_PROVINCIA.AsInteger := DataSet.FieldByName('CD_PROVINCIA').AsInteger;
  TB_PROTOCOLO_ENVIOCD_MUNICIPIO.AsInteger := DataSet.FieldByName('CD_MUNICIPIO').AsInteger;
  TB_PROTOCOLO_ENVIOVL_MESANO.AsString := DataSet.FieldByName('VL_MESANO').AsString;
    if FileExists(sArq) then
  TB_PROTOCOLO_ENVIOBL_PROTOCOLO.LoadFromFile(sArq);
  TB_PROTOCOLO_ENVIODT_EXP.Clear;
  TB_PROTOCOLO_ENVIOCD_EDI.Clear;

  if sOP = 'I' then
  begin
    TB_PROTOCOLO_ENVIOCD_USU_INCLUSAO.AsInteger := dm.qyLoginCD_USUARIO.AsInteger;
    TB_PROTOCOLO_ENVIODT_INCLUSAO.AsDateTime := dm.GetDate;
  end
  else if sOP = 'A' then
  begin
    TB_PROTOCOLO_ENVIOCD_USU_ALTERACAO.AsInteger := dm.qyLoginCD_USUARIO.AsInteger;
    TB_PROTOCOLO_ENVIODT_ALTERACAO.AsDateTime := dm.GetDate;
  end
  else if sOP = 'E' then
  begin
    TB_PROTOCOLO_ENVIOCD_USU_EXCLUSAO.AsInteger := dm.qyLoginCD_USUARIO.AsInteger;
    TB_PROTOCOLO_ENVIODT_EXCLUSAO.AsDateTime := dm.GetDate;
  end;

  TB_PROTOCOLO_ENVIO.Post;

  //if sOP <> 'E' then
  dm.Commit(TB_PROTOCOLO_ENVIO);
end;

