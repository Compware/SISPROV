procedure TDMGERAL.TB_PACIENTEBeforePost(DataSet: TDataSet);
begin
//
  if TB_PACIENTEFL_RESULTADO.AsString <> '' then
  if TB_PACIENTEFL_RESULTADO.AsInteger = cvPositivo then
    TB_PACIENTEFL_NOTIFICACAO.AsInteger := cvFlSim;

  if TB_PACIENTEDT_OBITO.AsString = '30/12/1899' then
    TB_PACIENTEDT_OBITO.Clear;

  if TB_PACIENTEFL_OBITO.AsInteger = 2 then
    BEGIN
      IF NOT (TB_NOTIFICACAO.State IN [dsEdit, dsInsert]) THEN
        BEGIN
          TB_NOTIFICACAO.Open;
          IF TB_NOTIFICACAO.Locate('CD_PACIENTE', TB_PACIENTECD_PACIENTE.AsInteger,[]) THEN
            BEGIN
              TB_NOTIFICACAO.Edit;
              TB_NOTIFICACAOCD_EVOLUCAO.AsInteger := 2;
              TB_NOTIFICACAODT_EVOLUCAO.AsDateTime := TB_PACIENTEDT_OBITO.AsDateTime;
              TB_NOTIFICACAO.Post;
            END;
          TB_NOTIFICACAO.Close;
        END
        ELSE
        BEGIN
          TB_NOTIFICACAOCD_EVOLUCAO.AsInteger := 2;
          TB_NOTIFICACAODT_EVOLUCAO.AsDateTime := TB_PACIENTEDT_OBITO.AsDateTime;
        END;
    END;

  //Deve incluir somente quando for feito agendamento
  {if (TB_PACIENTEFL_LAB.NewValue = Null) then
      dmLab.IncluiPaciente(NUMERO_INDEFINIDO);}
end;

