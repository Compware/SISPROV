procedure TDMGERAL.DeleteCascadeNotificacao;
   procedure Excluir(sTabela : string);
   begin
      try
        dm.qyGeral.Sql.Clear;
        dm.qyGeral.Sql.Add('delete from ' + sTabela);
        dm.qyGeral.Sql.Add('where CD_FICHA = :CD_FICHA');
        dm.qyGeral.Sql.Add('and   CD_UND   = :CD_UND');
        dm.qyGeral.Prepare;
        dm.qyGeral.ParamByName('CD_FICHA').AsString :=
          TB_NOTIFICACAO.FieldByName('CD_FICHA').AsString;
        dm.qyGeral.ParamByName('CD_UND').AsString :=
          TB_NOTIFICACAO.FieldByName('CD_UND').AsString;
        dm.qyGeral.ExecSQL;
      except
        on E:Exception do
          ShowMessage(E.Message);
      end;
   end;
begin
  Excluir('ASPECTO_NOTIFICACAO');
  Excluir('CIRCUNCISAO');
  Excluir('DENTARIO');
  Excluir('EXCECAO_IMPORTAR');
  Excluir('INTERV_CIRURGICA');
  Excluir('PERFURACAO');
  Excluir('TATUAGEM');
  Excluir('TRANSFUSAO');
end;

procedure TDMGERAL.TB_NOTIFICACAOAfterInsert(DataSet: TDataSet);
begin
  //TB_MUNICIPIODS_PAIS.AsString := '';
  //TB_MUNICIPIODS_PROVINCIA.AsString := '';
  TB_NOTIFICACAOCD_USUARIO.AsInteger := 1;
  TB_NOTIFICACAOCD_FICHA.AsInteger := dm.GeraCodigo('NOTIFICACAO_CD_FICHA_SEQ');
  TB_NOTIFICACAOCD_NOTIFICACAO.AsInteger := DM.GeraCodigo('GEN_CD_NOTIFICACAO');

  TB_NOTIFICACAOCD_UNIDADE.AsString := prmUnidade;
  TB_NOTIFICACAODS_UNIDADE.AsString := sUnidadeDefautDescricao;
  TB_NOTIFICACAOCD_PROVINCIA_NOT.AsInteger := iProvincia;
  TB_NOTIFICACAODS_PROVINCIA_NOT.AsString := sProvinciaDefautDescricao;
  TB_NOTIFICACAOCD_MUNICIPIO.AsInteger := iMunicipio;
  TB_NOTIFICACAODS_MUNICIPIO_NOT.AsString := sMunicipioDefautDescricao;

  TB_NOTIFICACAOCD_UNIDADE_TRATAMENTO.AsString := prmUnidade;
  TB_NOTIFICACAODS_UNIDADE_TRATAMENTO.AsString := sUnidadeDefautDescricao;
  TB_NOTIFICACAOCD_PROVINCIA_TRATAMENTO.AsInteger := iProvincia;
  TB_NOTIFICACAODS_PROVINCIA_TRATAMENTO.AsString := sProvinciaDefautDescricao;
  TB_NOTIFICACAOCD_MUNICIPIO_TRATAMENTO.AsInteger := iMunicipio;
  TB_NOTIFICACAODS_MUNICIPIO_TRATAMENTO.AsString := sMunicipioDefautDescricao;

  TB_NOTIFICACAO.FieldByName('CD_UND').AsString := prmUnidade;

  if TB_PACIENTE.State in [dsEdit, dsInsert] then
    TB_PACIENTEFL_RESULTADO.AsInteger := 1;
end;

