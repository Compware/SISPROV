procedure TDMGERAL.TB_USUARIOAfterInsert(DataSet: TDataSet);
begin
  //Empresa Principal
{  TB_USUARIOCD_EMPRESA.AsInteger := qyDadosEmpresaCD_EMPRESA.AsInteger;
  TB_USUARIOCD_USUARIO.AsInteger := dm.GeraCodigo('GEN_USUARIO');
  TB_USUARIOCD_EDI.AsInteger := 0;
  TB_USUARIOTP_EDI.AsString := 'I';
}
  TB_USUARIOCD_USUARIO.AsInteger := dm.GeraCodigo('USUARIO_CD_USUARIO_SEQ');
  TB_USUARIO.FieldByName('CD_UND').AsString := prmUnidade;

end;

procedure TDMGERAL.DataModuleCreate(Sender: TObject);
begin
  //Parametros
  InicializaParametros;
  //Provincia e Municipio vinculados a UnidadeDefaut
  qyUnidade.Open;
  iProvincia := qyUnidadeCD_PROVINCIA.AsInteger;
  iMunicipio := qyUnidadeCD_Municipio.AsInteger;
  sProvinciaDefautDescricao := qyUnidadeDS_PROVINCIA.AsString;
  sMunicipioDefautDescricao := qyUnidadeDS_Municipio.AsString;
  qyUnidade.Close;

  InicializaDados;

  LogSQL := TStringList.Create;
end;

