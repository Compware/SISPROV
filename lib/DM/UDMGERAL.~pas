procedure TDMGERAL.TB_AGENDAFL_CONFIRMADOValidate(Sender: TField);
var Data : TDateTime;
begin
  Data := dm.GetDate;

  if (TB_AGENDAFL_CONFIRMADO.AsInteger = cvFlSim)and
     (TB_AGENDADT_AGENDA.AsDateTime <> Data) then
  begin
     ShowMessage('A Confirmao s pode ser relizada na data da consulta!');
     Abort;
  end;
end;

