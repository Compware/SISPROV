procedure TDMGERAL.TrataErroDelete(DataSet: TDataSet;
  E: EDatabaseError; var Action: TDataAction);
var sErro, sNome, sTabela, sComplemento:String; bCopia:boolean;

  procedure PegaNomeTabela(msg:String);
  var i,j:integer;
  begin
    sTabela := '';
    i := Pos('on table',msg)+8;
    if (i > 8) then
      for j:=i to length(msg) do
      begin
        if not bCopia and (msg[j] = '"') then
        begin
          bCopia := true;
          Continue;
        end;

        if bCopia and (msg[j] = '"') then
           exit;

        if bCopia then
          sTabela := sTabela + msg[j];
      end;
  end;
begin
//'violation of FOREIGN KEY constraint "FK_TATUAGEM_REFERENCE_NOTIFICA" on table "TATUAGEM"'
//'Foreign key references are present for the record'
  if Pos('FOREIGN KEY',e.Message)>0 then
  begin
    PegaNomeTabela(e.Message);
    //Tabela
    if sTabela = 'NOTIFICACAO' then sComplemento := 'notificao';
    if sTabela = 'TATUAGEM'    then sComplemento := 'tatuagem';
    //Referencia
    if DataSet.Name = 'TB_PACIENTE'    then  sNome := 'paciente';
    if DataSet.Name = 'TB_NOTIFICACAO' then  sNome := 'notificao';
    //Mensagem
    sComplemento := sNome + ',pois existe uma associao com ' + sComplemento + '!';
  end;
  sErro := 'No foi possvel excluir ' + sComplemento;
  Application.MessageBox(pchar(sErro),'Erro',MB_ICONERROR);
  Action := daAbort;
end;

procedure TDMGERAL.TB_PARAMETRODeleteError(DataSet: TDataSet;
  E: EDatabaseError; var Action: TDataAction);
begin
  TrataErroDelete(DataSet, E, Action);
end;

